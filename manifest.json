{
    "manifest_version": 3,
    "name": "Chron",
    "version": "1.0.0",
    "action": { "default_popup": "index.html" },
    "permissions": [
        "scripting",
        "tabs",
        "readingList",
        "storage"
    ],
    "host_permissions":[
        "https://*/*",
        "http://*/*"
    ]
}